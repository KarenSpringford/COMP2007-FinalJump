</head>
<body>
    <!--HEADER NAVIGATION-->
    <header class="header">
        <div class="container">
            <div class="clear-float">              
                <a href="@Url.Action("Index", "Home")" class="navbar-brand"><img alt="Brand" id="brand" src="~/Assets/images/logo.jpg" /></a>
            </div><!-- CLEAR FLOAT -->
            <!-- MAIN MENU -->
            <nav id="header-nav">
                <div class="navbar-collapse collapse">
                    @Html.Partial("_LoginPartial")
                    <ul class="nav navbar-nav navbar-right">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("SkyDiveGear", "SkyDiveGear", "SkyDiveGear")</li>
                        <li>@Html.ActionLink("Clothing", "Clothing", "Clothing")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Contact")</li>
                    </ul>
                </div>
            </nav><!--.header-nav-->
            <div class="clear-float"></div><!--CLEAR FLOAT-->
        </div>
    </header>

        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - FINAL Group Project - FinalJump</p>
            </footer>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
    </body>
</html>
